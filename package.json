{
  "name": "zadif-api-posts",
  "version": "1.0.0",
  "main": "lambda.js",
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "sls-offline": "tsc && sls offline --useChildProcesses",
    "start": "npm-run-all -p watch sls-offline",
    "test": "",
    "e2e": "",
    "docker:install": "docker-compose -f ./docker/docker-compose.start.yml run --rm api npm install",
    "docker:test": "docker-compose -f ./docker/docker-compose.start.yml run --rm api npm run test",
    "docker:start": "docker-compose -f ./docker/docker-compose.start.yml up",
    "docker:e2e": "docker-compose -f ./docker/docker-compose.e2e.yml up"
  },
  "dependencies": {
    "@apollo/federation": "^0.16.0",
    "apollo-server-lambda": "^2.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.5",
    "aws-lambda": "^1.0.6",
    "npm-run-all": "^4.1.5",
    "serverless-offline": "^6.1.7",
    "serverless-webpack": "^5.3.3",
    "webpack": "^4.44.1"
  }
}
